{
  "name": "delovrukah",
  "private": true,
  "scripts": {
    "postinstall": "pnpm dotenv -- pnpm prisma generate",
    "build": "turbo build",
    "predev": "pnpm db:generate",
    "dev": "turbo dev",
    "lint": "turbo run lint --parallel -- --max-warnings=0 --cache",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:seed": "pnpm prisma generate && pnpm dotenv -- pnpm prisma db seed",
    "db:migrate": "pnpm prisma generate && pnpm dotenv -- pnpm prisma migrate deploy",
    "db:reset": "pnpm prisma generate && pnpm dotenv -- pnpm prisma migrate reset",
    "db:studio": "pnpm dotenv -- pnpm prisma studio",
    "db:generate": "pnpm dotenv -- pnpm prisma generate"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "bcrypt": "^5.1.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.38.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.20.0"
  },
  "prisma": {
    "seed": "pnpm ts-node prisma/seed.ts"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "transliteration": "^2.3.5"
  }
}
